function cancelEvent(id){
	var r = confirm("Are you sure cancel this event?");
	if(r == true){
		var cancelJson = '{"eventId":"'+id+'"}';
		$(document).ready(function(){
			$.ajax({
				url: '/ShopperWeb/CancelEventServlet?json='+cancelJson,
				type: 'POST',
				dataType: 'json',
				error: function(err){
					alert("cancel event ajax error!");
				},
				success: function(data){
					console.log(data);
					var status = data["status"];
					var message = data["message"];
					if(status == 0){
						alert(message);
					}else{
						//success
						backToMine();
					}
				}
			});
		});
	}else{
		
	}
}